{
  "name": "universal-redux-jwt",
  "version": "0.0.1",
  "description": "JWT example with Universal Redux",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build",
    "dev-server": "better-npm-run dev-server",
    "api-server": "better-npm-run api-server",
    "auth-server": "better-npm-run auth-server",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run api-server\" \"npm run auth-server\" \"npm run dev-server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-client": "better-npm-run watch-client"
  },
  "betterScripts": {
    "dev-server": {
      "command": "node ./bin/start",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "api-server": {
      "command": "node ./bin/start_api",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3030
      }
    },
    "auth-server": {
      "command": "node ./bin/start_auth",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3060
      }
    },
    "build": {
      "command": "universal-redux-build",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "watch-client": {
      "command": "universal-redux-watch",
      "env": {
        "NODE_PATH": "./src",
        "UV_THREADPOOL_SIZE": 100
      }
    }
  },
  "author": {
    "name": "Buck DeFore"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.2",
    "jsonwebtoken": "^5.4.1",
    "superagent": "^1.6.1",
    "universal-redux": "^0.17.2"
  },
  "devDependencies": {
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-plugin-typecheck": "^2.0.0",
    "babel-runtime": "~5.8.29",
    "better-npm-run": "0.0.5",
    "classnames": "^2.2.0",
    "concurrently": "1.0.0",
    "deep-equal": "^1.0.1",
    "eslint": "1.8.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "express-jwt-proxy": "^0.5.2",
    "flexboxgrid": "^6.3.0",
    "invariant": "^2.2.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-addons-css-transition-group": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "4.0.0",
    "react-toolbox": "^0.13.4",
    "react-tools": "^0.10.0",
    "react-transform-catch-errors": "^1.0.0",
    "redbox-react": "^1.1.1",
    "redux-router": "1.0.0-beta4",
    "url-loader": "^0.5.7",
    "warning": "^2.1.0"
  }
}
